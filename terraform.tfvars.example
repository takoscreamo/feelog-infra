# Cloudflare settings
cloudflare_api_token = "your-api-token-here"
cloudflare_zone_id   = "your-zone-id-here"

# AWS settings
aws_region = "ap-northeast-1"

# Lambda settings
lambda_function_name = "tofunote-backend"
lambda_timeout       = 30
lambda_memory_size   = 512

# Database settings (Neon)
db_host     = "your-neon-host.neon.tech"
db_port     = "5432"
db_user     = "your-db-user"
db_password = "your-db-password"
db_name     = "tofunote"

# Environment
env = "prod"

# CORS settings
cors_origin = "https://tofunote.takoscreamo.com"

# Vercel settings
vercel_api_token = "your-vercel-api-token-here"
vercel_project_id = "your-vercel-project-id-here"
# vercel_team_id = "your-vercel-team-id-here"  # オプション

openrouter_api_key = "your-openrouter-api-key"
jwt_secret = "your-jwt-secret"
